\n          languageCallback={this.handleLanguageClick}\n        />\n        {this.state.repo_data.map((repo) => (\n          <RepoItem key={repo.id} dataRepoItem={repo} />\n        ))}\n      </div>\n    );\n  }\n}\n\nexport { ShowRepo };\n",["40","41"],"/Users/tansinjahan/Documents/intermediate-dev-2021-01/web/src/component/RepoItem.js",[],"/Users/tansinjahan/Documents/intermediate-dev-2021-01/web/src/component/LanguageItem.js",[],"/Users/tansinjahan/Documents/intermediate-dev-2021-01/web/src/component/AppRoutes.js",[],"/Users/tansinjahan/Documents/intermediate-dev-2021-01/web/src/component/Displayrepo.js",[],"/Users/tansinjahan/Documents/intermediate-dev-2021-01/web/src/component/DisplayRepo.js",[],{"ruleId":"42","replacedBy":"43"},{"ruleId":"44","replacedBy":"45"},{"ruleId":"46","severity":1,"message":"47","line":49,"column":9,"nodeType":"48","messageId":"49","endLine":49,"endColumn":20},{"ruleId":"42","replacedBy":"43"},{"ruleId":"44","replacedBy":"45"},"no-native-reassign",["50"],"no-negated-in-lhs",["51"],"no-console","Unexpected console statement.","MemberExpression","unexpected","no-global-assign","no-unsafe-negation"]